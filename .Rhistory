g + geom_bar(stat='identity') + labs(x = "Model", y = "Frequency") +
geom_text(aes(label=frequencies), position=position_dodge(width=0.9),
vjust=-0.25) + labs(title = "Top 11 Models",
subtitle ="These are the top 11 truck models being used in the U.S.")
